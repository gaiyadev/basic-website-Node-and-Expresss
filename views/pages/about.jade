      #colorlib-consult
        .video.colorlib-video(style='background-image: url(images/video.jpg);', data-stellar-background-ratio='0.5')
        .choose.choose-form.animate-box
          .colorlib-heading
            h2 Free Legal Consultation
          form(action='#')
            .row.form-group
              .col-md-6
                // <label for="fname">First Name</label>
                input#fname.form-control(type='text', placeholder='Your firstname')
              .col-md-6
                // <label for="lname">Last Name</label>
                input#lname.form-control(type='text', placeholder='Your lastname')
            .row.form-group
              .col-md-12
                // <label for="email">Email</label>
                input#email.form-control(type='text', placeholder='Your email address')
            .row.form-group
              .col-md-12
                // <label for="subject">Subject</label>
                input#subject.form-control(type='text', placeholder='Your subject of this message')
            .row.form-group
              .col-md-12
                // <label for="message">Message</label>
                textarea#message.form-control(name='message', cols='30', rows='10', placeholder='Say something about us')
            .form-group
              input.btn.btn-primary(type='submit', value='Send Message')
